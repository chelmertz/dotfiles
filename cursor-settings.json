{
    "editor.fontFamily": "'Go Mono', Inconsolata,'Droid Sans Mono','Fira Mono', 'monospace', monospace",
    "editor.fontSize": 16,
    "explorer.confirmDragAndDrop": false,
    "files.autoSave": "onWindowChange",
    "github.copilot.enable": {
        "*": true,
        "markdown": false,
        "plaintext": false,
        "yaml": false
    },
    "gitlens.defaultDateFormat": null,
    "gitlens.defaultDateLocale": "sv-SE",
    "gitlens.defaultDateShortFormat": null,
    "gitlens.defaultTimeFormat": null,
    "go.autocompleteUnimportedPackages": true,
    "go.toolsManagement.autoUpdate": true,
    "json.schemas": [],
    "redhat.telemetry.enabled": true,
    "sqltools.useNodeRuntime": true,
    "workbench.iconTheme": "material-icon-theme",
    "yaml.customTags": [
        "!And",
        "!And sequence",
        "!If",
        "!If sequence",
        "!Not",
        "!Not sequence",
        "!Equals",
        "!Equals sequence",
        "!Or",
        "!Or sequence",
        "!FindInMap",
        "!FindInMap sequence",
        "!Base64",
        "!Join",
        "!Join sequence",
        "!Cidr",
        "!Ref",
        "!Sub",
        "!Sub sequence",
        "!GetAtt",
        "!GetAZs",
        "!ImportValue",
        "!ImportValue sequence",
        "!Select",
        "!Select sequence",
        "!Split",
        "!Split sequence"
    ],
    "workbench.editor.revealIfOpen": true,
    "terminal.integrated.scrollback": 100000,
    "[python]": {
        "editor.formatOnType": true
    },
    "vim.useCtrlKeys": true,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "u"
            ],
            "commands": [
                "undo"
            ]
        },
        {
            "before": [
                "<C-r>"
            ],
            "commands": [
                "redo"
            ]
        }
    ],
    "editor.inlineSuggest.enabled": true,
    "window.zoomLevel": 2,
    "workbench.colorTheme": "No Happiness in Colors Theme",
    "workbench.preferredDarkColorTheme": "No Happiness in Colors Theme",
    "workbench.preferredLightColorTheme": "Subtle Monochrome (Light)",
    "window.autoDetectColorScheme": true,
    "githubPullRequests.terminalLinksHandler": "github",
    "editor.fontWeight": "normal",
    "yaml.maxItemsComputed": 10000,
    "githubPullRequests.pullBranch": "never",
    "git.openRepositoryInParentFolders": "never",
    "diffEditor.hideUnchangedRegions.enabled": true,
    "explorer.confirmDelete": false,
    "vim.visualModeKeyBindings": [
        {
            "before": [
                "<"
            ],
            "after": [
                "<",
                "g",
                "v"
            ]
        },
        {
            "before": [
                ">"
            ],
            "after": [
                ">",
                "g",
                "v"
            ]
        }
    ],
    "rust-analyzer.procMacro.enabled": false,
    "rust-analyzer.server.extraEnv": {
        "DATABASE_URL": "postgres://pb:pb@localhost:5432/padelboard"
    },
    "github.copilot.editor.enableAutoCompletions": true,
    "workbench.sideBar.location": "right",
    "go.lintTool": "golangci-lint",
    "go.lintFlags": [
        "--fast"
    ],
    "cursor.cpp.disabledLanguages": [
        "markdown",
        "plaintext",
        "yaml"
    ],
    "vale.doNotShowWarningForFileToBeSavedBeforeLinting": true,
    "gitlens.advanced.messages": {
        "suppressFileNotUnderSourceControlWarning": true
    },
    "editor.bracketPairColorization.enabled": false,
    "markdown.preview.scrollEditorWithPreview": false,
    "markdown.preview.scrollPreviewWithEditor": false,
    "msg.editor.defaultDbConnection": "webapp db",
    "[dockercompose]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 2,
        "editor.autoIndent": "advanced",
        "editor.quickSuggestions": {
            "other": true,
            "comments": false,
            "strings": true
        },
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "[github-actions-workflow]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "workbench.tree.indent": 24,
    "go.inlayHints.assignVariableTypes": true,
    "go.inlayHints.compositeLiteralFields": true,
    "go.inlayHints.compositeLiteralTypes": true,
    "go.inlayHints.constantValues": true,
    "go.inlayHints.ignoredError": true,
    "go.inlayHints.rangeVariableTypes": true,
    "go.inlayHints.parameterNames": true,
    "claudeCode.preferredLocation": "sidebar"
}