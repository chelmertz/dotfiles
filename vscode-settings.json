{
    "editor.fontFamily": "Iosevka,Inconsolata,'Droid Sans Mono','Fira Mono', 'monospace', monospace",
    "editor.fontSize": 16,
    "explorer.confirmDragAndDrop": false,
    "files.autoSave": "onWindowChange",
    "github.copilot.enable": {
        "*": true,
        "markdown": false,
        "plaintext": false,
        "yaml": false
    },
    "gitlens.defaultDateFormat": null,
    "gitlens.defaultDateLocale": "sv-SE",
    "gitlens.defaultDateShortFormat": null,
    "gitlens.defaultTimeFormat": null,
    "go.autocompleteUnimportedPackages": true,
    "go.toolsManagement.autoUpdate": true,
    "json.schemas": [],
    "redhat.telemetry.enabled": true,
    "sqltools.useNodeRuntime": true,
    "workbench.iconTheme": "material-icon-theme",
    "yaml.customTags": [
        "!And",
        "!And sequence",
        "!If",
        "!If sequence",
        "!Not",
        "!Not sequence",
        "!Equals",
        "!Equals sequence",
        "!Or",
        "!Or sequence",
        "!FindInMap",
        "!FindInMap sequence",
        "!Base64",
        "!Join",
        "!Join sequence",
        "!Cidr",
        "!Ref",
        "!Sub",
        "!Sub sequence",
        "!GetAtt",
        "!GetAZs",
        "!ImportValue",
        "!ImportValue sequence",
        "!Select",
        "!Select sequence",
        "!Split",
        "!Split sequence"
    ],
    "workbench.editor.revealIfOpen": true,
    "terminal.integrated.scrollback": 100000,
    "[python]": {
        "editor.formatOnType": true
    },
    "vim.useCtrlKeys": true,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": ["u"],
            "commands": ["undo"]
        },
        {
            "before": ["<C-r>"],
            "commands": ["redo"]
        }
    ],
    "editor.inlineSuggest.enabled": true,
    "window.zoomLevel": 2,
    "githubPullRequests.terminalLinksHandler": "github",
    "editor.fontWeight": "600",
    "yaml.maxItemsComputed": 10000,
    "githubPullRequests.pullBranch": "never",
    "git.openRepositoryInParentFolders": "never",
    "diffEditor.hideUnchangedRegions.enabled": true,
    "explorer.confirmDelete": false,
    "vim.visualModeKeyBindings": [
        
        {
            "before": ["<"],
            "after": ["<", "g", "v"]
        },
        {
            "before": [">"],
            "after": [">", "g", "v"]
        }
    ],
    "rust-analyzer.procMacro.enabled": false,
    "github.copilot.editor.enableAutoCompletions": true,
    "workbench.sideBar.location": "right",
    "go.lintTool": "golangci-lint",
    "go.lintFlags": [
        "--fast"
    ],
    "workbench.colorTheme": "Subtle Monochrome (Light)",
    "editor.bracketPairColorization.enabled": false,
    "vale.doNotShowWarningForFileToBeSavedBeforeLinting": true
}
